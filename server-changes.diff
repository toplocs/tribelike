+        const hash = CryptoJS.SHA256(item.userId).toString(CryptoJS.enc.Hex);
+        const magiclink: IMagicLink = {
+            id: '',
+            token: hash,
+            userId: item.userId,
+            expires: new Date(Date.now()+10*60*1000)
+        };
+        return super.create(magiclink);
