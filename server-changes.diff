+                console.warn(`File ${id}.jsonl does not exist for ${this.dataPath}`);
+                return false;
+            }
+        } catch (err) {
+            console.error(`Error deleting item ${id} for ${this.dataPath}:`, err);
+            return false;
+        }
+    }
+}
